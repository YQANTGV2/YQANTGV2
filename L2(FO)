getgenv().FakeOutEnabled = false
if getgenv()._Connection then
   getgenv()._Connection:Disconnect()
   getgenv()._Connection = nil
end

-- Reset camera
local cam = workspace.CurrentCamera
local plr = game:GetService("Players").LocalPlayer
cam.CameraSubject = plr.Character:FindFirstChildOfClass("Humanoid") or plr.Character
cam.CameraType = Enum.CameraType.Custom
getgenv()._FakeOutDone = false
